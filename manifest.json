{
  "manifest_version": 2,

  "name": "SIU Web Clipper",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [ {
      // "css": [  ],
      "js": [ "js/clipper.js" ],
      "matches": [ "*://*/*" ],
      "run_at": "document_start"
  } ],
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}